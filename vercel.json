{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "PHP_INI_SCAN_DIR": ".:/var/task/user/php/conf.d"
  }
}
